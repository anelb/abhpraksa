<nav class="navbar">
  <ul class="nav navbar-nav">
    <li><%= link_to 'abhShoes', root_path %></li>
    <% Category.all.each do |category| %>
      <li><%= link_to category.title, '#' %></li>
    <% end %>
    <% if current_user %>
      <% if current_user.role == 'admin' %>
        <li><%= link_to 'ADMIN', admin_users_path %>
      <% end %>
      <li><%= link_to 'LOG OUT', logout_path, method: 'delete' %></li>
    <% else %>
      <li><%= link_to 'LOGIN', sign_in_path %></li>
      <li><%= link_to 'REG', sign_up_path %></li>
    <% end %>
  </ul>
</nav>


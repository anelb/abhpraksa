<div class="row">
  <table class='table table-bordered table-hover'>
    <thead>
      <tr>
        <th>ID</th>
        <th>Proizvod</th>
        <th>Boja</th>
        <th>Velicina</th>
        <th>Kolicina</th>
        <th>Cijena</th>
      </tr>
